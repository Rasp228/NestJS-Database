<div class="container">
    <table class="table">
        <tr>
            <th>ID</th>
            <th>Gwiazda</th>
            <th>Nazwa</th>
            <th>Typ</th>
            <th>Rzadkosc</th>
            <th>Teren</th>
            <th>Flora</th>
            <th>Fauna</th>
            <th>Straznicy</th>
            <th>Akcje</th>
        </tr>
            {{#each planety}}
                <tr>  
                    <td>{{this.ID}}</td>
                    <td>{{this.ID_Gwiazdy}}</td>
                    <td>{{this.Nazwa}}</td>
                    <td>{{this.Typ}}</td>
                    <td>{{this.Rzadkosc}}</td>
                    <td>{{this.Teren}}</td>
                    <td>{{this.Flora}}</td>
                    <td>{{this.Fauna}}</td>
                    <td>{{this.Straznicy}}</td>
                    {{ user.isAdmin }}
                    {{#if ../user.isAdmin}}
                    <td><a href="edit/{{this.ID}}">Edytuj</a> <a href="delete/{{this.ID}}">Usu≈Ñ</a></td>
                    {{/if}}
                </tr>
            {{/each}} 
    </table>
    <a href="add/">Dodaj</a>
</div>